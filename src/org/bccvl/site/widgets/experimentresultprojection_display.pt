<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      tal:omit-tag="">
  <div id="" class=""
       tal:attributes="id view/id;
                       class view/klass;
                       style view/style;
                       title view/title;
                       lang view/lang;
                       onclick view/onclick;
                       ondblclick view/ondblclick;
                       onmousedown view/onmousedown;
                       onmouseup view/onmouseup;
                       onmouseover view/onmouseover;
                       onmousemove view/onmousemove;
                       onmouseout view/onmouseout;
                       onkeypress view/onkeypress;
                       onkeydown view/onkeydown;
                       onkeyup view/onkeyup">
    <tal:block tal:repeat="item view/items">
      <div id="" class="row">
        <span tal:content="item/title">Experiment Title</span>
        <tal:block tal:repeat="dataset item/datasets">
          <span tal:condition="dataset/selected"
                tal:content="dataset/title">Dataset Title</span>
	  <span tal:condition="dataset/selected"
		tal:content="dataset/threshold">Threshold value</span>
        </tal:block>
      </div>
    </tal:block>
  </div>
</html>
