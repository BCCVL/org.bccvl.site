<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="plone.z3cform"
      tal:omit-tag="">

  <head></head>

  <body>

    <metal:block metal:define-macro="datasets_tabs">

    <ul class="contentTabs">
        <li class="contentTab firstContentTab"
               tal:define="selected python: view.__name__ == 'datasets_listing_view';
                           selected python: 'selected' if selected else ''"
               tal:attributes="class string:contentTab firstContentTab ${selected}">
            <a id="datasets-listing"
               href="#"
               tal:attributes="href string:${context/absolute_url}"><span>My Datasets</span></a>
        </li>
        <li class="contentTab"
            tal:define="selected python: view.__name__ == 'datasets_import_view';
                           selected python: 'selected' if selected else ''"
               tal:attributes="class string:contentTab ${selected}">
            <a id="datasets-import"
               href="#"
               tal:attributes="href string:${context/absolute_url}/datasets_import_view"><span>Import Dataset</span></a>
        </li>
        <li class="contentTab lastContentTab"
               tal:define="selected python: view.__name__ == 'datasets_upload_view';
                           selected python: 'selected' if selected else ''"
               tal:attributes="class string:contentTab lastContentTab ${selected}">
            <a id="datasets-upload"
               href="#"
               tal:attributes="href string:${context/absolute_url}/datasets_upload_view"><span>Upload Dataset</span></a>
        </li>
    </ul>

    </metal:block>
  </body>
</html>
