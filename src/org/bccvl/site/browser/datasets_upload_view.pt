<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

<body>

<metal:content-core fill-slot="body">

    <tal:tabs metal:use-macro="context/datasets-macros/macros/datasets_tabs"></tal:tabs>

    <div class="bccvl-add-datasets tab-pane active" id="tab-find">
        <div class="row-fluid">
	    <div class="span6 offset3">

              <div class="bccvlbutton-dataset-upload">
                  <tal:form tal:define="view nocall:view/addspeciesform">"
                    <metal:block
                        use-macro="context/@@ploneform-macros/titlelessform"
                        />
                  </tal:form>

                  <tal:form tal:define="view nocall:view/addlayerform">"
                    <metal:block
                        use-macro="context/@@ploneform-macros/titlelessform"
                        />
                  </tal:form>

                  <a class="btn btn-primary bccvlbutton-dataset-upload pull-left" href="http://localhost:8499/bccvl/datasets/++add++org.bccvl.content.dataset">Upload New Dataset</a>
              </div>
            </div>

        </div>
    </div>


</metal:content-core>

</body>
</html>
