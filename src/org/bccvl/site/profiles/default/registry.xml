  <record name="gu.plone.rdf.interfaces.IRDFSettings.fresnel_graph_uri" interface="gu.plone.rdf.interfaces.IRDFSettings" field="fresnel_graph_uri">
    <field type="plone.registry.field.URI">
      <title>Fresnel Graph URI</title>
    </field>
    <value>http://namespaces.gu.edu.au/z3cform/fresnel#</value>
  </record>
